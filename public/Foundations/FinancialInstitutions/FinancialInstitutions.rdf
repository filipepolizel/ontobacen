    <!-- https://ontobacen.web.app/Foundations/FinancialInstitutions/InstitutionAuthorizedToOperateByBrazilCentralBank -->

    <owl:Class rdf:about="https://ontobacen.web.app/Foundations/FinancialInstitutions/InstitutionAuthorizedToOperateByBrazilCentralBank">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LEIEntities/ContractuallyCapableEntity"/>
        <rdfs:label xml:lang="pt">instituição autorizada a funcionar pelo Banco Central do Brasil</rdfs:label>
        <rdfs:label>institution authorized to operate by Brazil Central Bank</rdfs:label>
        <skos:definition>an entity legally and financially responsible for the performance of financial transactions, and authorized to operate over Brazil Central Bank&apos;s jurisdiction (Brazilian Financial System). This excludes natural persons, but includes private and governmental financial institutions and organizations.</skos:definition>
    </owl:Class>
    


